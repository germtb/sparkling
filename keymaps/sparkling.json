{
	"body atom-workspace": {
		"cmd-p": "sparkling:files",
		"cmd-m": "sparkling:gitFiles",
		"cmd-b": "sparkling-extras:gitBranches",
		"cmd-g": "sparkling:ls"
	},
	"body atom-workspace atom-text-editor:not([mini])": {
		"cmd-l": "sparkling:lines",
		"cmd-shift-l": "sparkling:allLines"
	},
	"body atom-workspace .sparkling": {
		"alt-k": "sparkling:next",
		"alt-j": "sparkling:previous",
		"escape": "sparkling:hide",
		"enter": "sparkling:accept"
	}
}
