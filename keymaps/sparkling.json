{
	".platform-darwin atom-workspace": {
		"cmd-l cmd-s": "sparkling:ls",
		"cmd-shift-a": "sparkling:findToggle",
		"cmd-r cmd-p": "sparkling:removeFiles",
		"cmd-d cmd-p": "sparkling:duplicateFiles",
		"cmd-r cmd-r": "sparkling:renameFiles"
	},
	".platform-darwin .sparkling-files": {
		"cmd-p": "sparkling:files"
	},
	".platform-darwin .sparkling-gitFiles": {
		"cmd-g cmd-p": "sparkling:gitFiles",
		"cmd-g cmd-s": "sparkling:gitStage"
	},
	".platform-darwin .sparkling-gitBranches": {
		"cmd-g cmd-b": "sparkling:gitBranches"
	},
	".platform-darwin .sparkling-lines atom-text-editor": {
		"cmd-l cmd-l": "sparkling:lines"
	},
	".platform-darwin .sparkling-allLines": {
		"cmd-l cmd-L": "sparkling:allLines"
	},
	".platform-darwin .sparkling-autocompleteLines atom-text-editor": {
		"cmd-l cmd-a": "sparkling:autocompleteLines"
	},
	".platform-darwin .sparkling-input": {
		"ctrl-k": "sparkling:previous",
		"ctrl-l": "sparkling:right",
		"ctrl-h": "sparkling:left",
		"ctrl-j": "sparkling:next",
		"up": "sparkling:previous",
		"right": "sparkling:right",
		"left": "sparkling:left",
		"down": "sparkling:next",
		"escape": "sparkling:hide",
		"cmd-escape": "sparkling:hide",
		"enter": "sparkling:accept"
	},
	".sparkling-find.sparkling-input": {
		"enter": "sparkling:find",
		"shift-enter": "sparkling:replace"
	},
	"#sparking-duplicate-file-confirm .sparkling-input": {
		"enter": "sparkling:duplicateFilesConfirm"
	},
	"#sparking-rename-file-confirm .sparkling-input": {
		"enter": "sparkling:renameFilesConfirm"
	},
	"#sparkling-ls .sparkling-input": {
		"cmd-l cmd-s": "sparkling:lsUp"
	},

	".platform-win32 atom-workspace, .platform-linux .sparkling-input": {
		"ctrl-l ctrl-s": "sparkling:ls",
		"ctrl-shift-a": "sparkling:findToggle",
		"ctrl-r ctrl-p": "sparkling:removeFiles",
		"ctrl-d ctrl-p": "sparkling:duplicateFiles"
	},
	".platform-win32 .sparkling-files, .platform-linux .sparkling-input": {
		"ctrl-p": "sparkling:files"
	},
	".platform-win32 .sparkling-gitFiles, .platform-linux .sparkling-input": {
		"ctrl-g ctrl-p": "sparkling:gitFiles",
		"ctrl-g ctrl-s": "sparkling:gitStage"
	},
	".platform-win32 .sparkling-gitBranches, .platform-linux .sparkling-input": {
		"ctrl-g ctrl-b": "sparkling:gitBranches"
	},
	".platform-win32 .sparkling-lines atom-text-editor, .platform-linux .sparkling-input": {
		"ctrl-l ctrl-l": "sparkling:lines"
	},
	".platform-win32 .sparkling-allLines, .platform-linux .sparkling-input": {
		"ctrl-l ctrl-L": "sparkling:allLines"
	},
	".platform-win32 .sparkling-autocompleteLines atom-text-editor, .platform-linux .sparkling-input": {
		"ctrl-l ctrl-a": "sparkling:autocompleteLines"
	},
	".platform-win32 .sparkling-input, .platform-linux .sparkling-input": {
		"ctrl-l ctrl-s": "sparkling:lsUp",
		"ctrl-k": "sparkling:previous",
		"ctrl-l": "sparkling:right",
		"ctrl-h": "sparkling:left",
		"ctrl-j": "sparkling:next",
		"up": "sparkling:previous",
		"right": "sparkling:right",
		"left": "sparkling:left",
		"down": "sparkling:next",
		"escape": "sparkling:hide",
		"ctrl-escape": "sparkling:hide",
		"enter": "sparkling:accept"
	}
}
