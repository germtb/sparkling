{
	"name": "sparkling",
	"main": "./dist/bundle",
	"version": "0.0.0",
	"activationEvents": [
		"sparkling:accept",
		"sparkling:next",
		"sparkling:previous",
		"sparkling:files"
	],
	"description": "Sparkling search like an olive in a gin and tonic",
	"scripts": {
		"test":
			"mocha --reporter progress --compilers js:babel-core/register --recursive 'lib/**/*.test.js'",
		"build": "webpack",
		"watch": "webpack --watch"
	},
	"keywords": ["fuzzy", "search", "bubbles"],
	"repository": "https://github.com/germtb/sparkling",
	"license": "MIT",
	"engines": {
		"atom": ">=1.0.0 <2.0.0"
	},
	"dependencies": {
		"classnames": "^2.2.5",
		"fuzzy": "^0.1.3",
		"react": "^15.5.3",
		"react-dom": "^15.5.3",
		"react-redux": "^5.0.4",
		"redux": "^3.6.0",
		"reselect": "^3.0.0",
		"xstream": "^11.2.0"
	},
	"devDependencies": {
		"babel-cli": "^6.26.0",
		"babel-core": "^6.24.1",
		"babel-eslint": "^7.2.3",
		"babel-loader": "^7.1.2",
		"babel-polyfill": "^6.23.0",
		"babel-preset-env": "^1.6.0",
		"babel-preset-es2015": "~6.22.0",
		"babel-preset-react": "^6.24.1",
		"eslint": "^3.19.0",
		"eslint-config-airbnb": "^14.1.0",
		"eslint-plugin-import": "^2.2.0",
		"eslint-plugin-jsx-a11y": "^4.0.0",
		"eslint-plugin-react": "^7.3.0",
		"webpack": "^3.5.5"
	}
}
