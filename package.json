  "description": "Sparkling is a general fuzzy finder so fuzzy that it is fizzy",
  "scripts": {
    "test": "jest",
    "build": "rollup -c"
  },
  "keywords": [
    "fuzzy",
    "find"
  ],
  "repository": "https://github.com/germtb/sparkling",
  "jest": {},
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
