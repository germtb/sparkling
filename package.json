{
  "name": "sparkling",
  "main": "./lib/sparkling",
  "version": "0.3.18",
  "activationEvents": [
    "sparkling:files",
    "sparkling:gitFiles",
    "sparkling:gitStage",
    "sparkling:gitBranches",
    "sparkling:gitLog",
    "sparkling:gitLogCheckout",
    "sparkling:gitCheckout",
    "sparkling:gitReflog",
    "sparkling:gitReflogCheckout",
    "sparkling:allLines",
    "sparkling:ls",
    "sparkling:removeFiles",
    "sparkling:copyFiles",
    "sparkling:moveFiles",
    "sparkling:findToggle",
    "sparkling:findInBufferToggle",
    "sparkling:relativePathInsert",
    "sparkling:relativePathCopy",
    "sparkling:lines",
    "sparkling:autocompleteLines"
